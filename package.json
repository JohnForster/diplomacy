{
  "scripts": {
    "build": "yarn webpack && yarn buildServer",
    "buildServer": "tsc src/server/* --outDir dist",
    "host": "yarn webpack --watch",
    "start": "yarn webpack-dev-server --mode=development --config ./webpack.config.js",
    "start:dev": "concurrently yarn startServerDev & yarn startClientDev",
    "startServerDev": "ts-node src/server/server.ts",
    "startClientDev": "yarn webpack-dev-server --mode=development --config ./webpack.config.js",
    "startServerLive": "node dist/server.js",
    "express": "yarn build && node server.js"
  },
  "dependencies": {
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "nodemon-webpack-plugin": "^4.0.8",
    "svg.js": "^2.7.1",
    "svgdom": "^0.0.18"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^11.13.7",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "nodemon": "^1.18.11",
    "source-map-loader": "^0.2.4",
    "svg-inline-loader": "^0.8.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.1.0",
    "tslint": "^5.15.0",
    "typescript": "^3.4.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  }
}
