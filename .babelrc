{
  "presets": [
    [
      "@babel/env"
    ],
    "@babel/typescript"
  ],
  "ignore": [
    "src/client/**/*"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "babel-plugin-module-resolver", {
        "root": ["."],
        "alias": {
          "@shared": "./src/shared",
          "@helpers": "./src/server/helpers",
          "@controllers": "./src/server/controllers",
          "@data": "./src/server/data",
          "@models": "./src/server/models",
          "@services": "./src/server/services"
        }
      }
    ],
  ]
}